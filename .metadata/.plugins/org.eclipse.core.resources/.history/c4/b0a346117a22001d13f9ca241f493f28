package unit02.Employee;

public class Master extends Employee{
	int bounus ;
	public Master(String no,String name ,int career,int bounus) {
		this.no = no ; 
		this.name = name ;
		this.career = career ;  
		this.bounus = bounus ; 
	}
	@Override
	public void paySalary() {
		// TODO Auto-generated method stub
		salary = career*30 + bounus ; 
	}
}
